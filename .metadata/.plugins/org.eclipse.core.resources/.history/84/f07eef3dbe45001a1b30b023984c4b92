from builtins import str
from numpy.core._multiarray_tests import format_float_OSprintf_g
print("哈喽沃德\nzhushoujun")

number_list = [1,2,3,4,45,5,5]
print(str(number_list) + "somethings")
print(number_list[2])
del number_list[2]
print(number_list[-2])
print(number_list[1:])
print("max:" + str(max(number_list)))
print("min:" + str(min(number_list)))
print("length:" + str(len(number_list)))
number_list.reverse()
print(number_list)
number_list.sort()
print(number_list)
index = number_list.index(5)
print(index)
number_list.append('abc')
print(number_list)
times = number_list.count(5)
print(times)