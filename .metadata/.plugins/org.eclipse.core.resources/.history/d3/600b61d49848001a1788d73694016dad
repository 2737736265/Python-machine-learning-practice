num = 88
for i in range(1,10)
    print("开始猜测ʼ\n")
    userGuess = int(input("输入你猜的数字:"))
    if userGuess == num:
        print("猜对了")  
        exit
    elif userGuess > num:
        print("猜大了")
    else:
        print("猜小了")
else:
    print("次数超限")